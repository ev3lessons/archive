var imgCanvas = document.getElementById('wPaint-canvas');
var imgAsDataURL = imgCanvas.toDataURL("image/png");

window.localStorage.canvasImage = imgAsDataURL;
}
function svimg() {
var imgCanvas = document.getElementById('wPaint-canvas');
var imgAsDataURL = imgCanvas.toDataURL("image/png");

window.localStorage.canvasImage = window.localStorage.canvasImage + '|' + imgAsDataURL;
}


function loadImgBg () {

  // internal function for displaying background images modal
  // where images is an array of images (base64 or url path)
  // NOTE: that if you can't see the bg image changing it's probably
  // becasue the foregroud image is not transparent.
  this._showFileModal('bg', images);
}

function loadImgFg () {

  /*  // internal function for displaying foreground images modal
  // where images is an array of images (base64 or url path)
  this._showFileModal('fg', images);
  */
  //alert('hi');
  ///    $("#imageLoader").click();
  document.getElementById('tmpimgholder').src =  window.localStorage.canvasImage;
  var c = document.getElementById("wPaint-canvas");
  var ctx = c.getContext("2d");
  var img = document.getElementById("tmpimgholder");
  ctx.drawImage(img,10,10);
    };



function loadsave(data) {
  document.getElementById('tmpimgholder').src =  data;
  var c = document.getElementById("wPaint-canvas");
  var ctx = c.getContext("2d");
  var img = document.getElementById("tmpimgholder");
  ctx.drawImage(img,0,0);

}




// init wPaint
$('#wPaint').wPaint({
  path: './',
  menuOffsetLeft: -35,
  menuOffsetTop: -50,
  saveImg: saveImg,
  loadImgBg: loadImgBg,
  loadImgFg: loadImgFg,
  //	  image: "trashtrek-field.jpg"
});
$(function() {
  $("#btnSave").click(function() {
    html2canvas($("#widget"), {
      onrendered: function(canvas) {
        theCanvas = canvas;
        document.body.appendChild(canvas);

        // Convert and download as image
        Canvas2Image.saveAsPNG(canvas);
        $("#img-out").append(canvas);
        // Clean up
        //document.body.removeChild(canvas);
      }
    });
  });
});



</script>
